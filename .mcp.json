{
  "mcpServers": {
    "alfa-sdui-mcp": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/Users/username/Scripts/alfa-sdui-mcp/dist/index.js"
      ],
      "env": {
        "PROJECT_ROOT": "/Users/username/Scripts/sdui-data_v2",
        "SDUI_BASE_PATH": "/Users/username/Scripts/sdui-data_v2/SDUI",
        "METASCHEMA_PATH": "/Users/username/Scripts/sdui-data_v2/metaschema/schema/strict_unversioned.json",
        "DEFAULT_PLATFORM": "web"
      },
      "alwaysAllow": [
        "check_component_availability",
        "find_alternative_component",
        "validate_contract_incremental",
        "convert_to_web",
        "apply_state_aware",
        "resolve_schema_ref",
        "build_contract_step",
        "get_web_components",
        "validate_stateaware"
      ]
    },
    "code-index": {
