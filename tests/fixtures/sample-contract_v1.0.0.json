{
  "type": "Screen",
  "version": "v1",
  "components": [
    {
      "type": "ButtonView",
      "text": "Click Me",
      "style": {
        "backgroundColor": "#007AFF",
        "textColor": "#FFFFFF",
        "cornerRadius": 8
      },
      "actions": [
        {
          "type": "navigate",
          "destination": "/next-screen"
        }
      ]
    },
    {
      "type": "TextView",
      "text": "Welcome to SDUI",
      "style": {
        "fontSize": 24,
        "fontWeight": "bold",
        "textColor": "#000000"
      }
    },
    {
      "type": "ImageView",
      "source": "https://example.com/logo.png",
      "style": {
        "width": 100,
        "height": 100,
        "contentMode": "fit"
      }
    }
  ],
  "dataBindings": [
    {
      "type": "state",
      "key": "userName",
      "defaultValue": "Guest"
    },
    {
      "type": "data",
      "key": "userProfile",
      "endpoint": "/api/user/profile"
    }
  ],
  "metadata": {
    "title": "Main Screen",
    "description": "Sample SDUI contract for testing position tracking",
    "version": "1.0.0"
  }
}
